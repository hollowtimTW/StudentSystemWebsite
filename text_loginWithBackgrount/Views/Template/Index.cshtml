@{
    ViewData["Title"] = "首頁";
}
<!-- ========================================================
       Hero Section 注目區
========================================================= -->
@section HeroSection {
    <section id="hero" class="hero">
        <div class="container position-relative">
            <div class="row gy-5" data-aos="fade-in">
                <div class="col-lg-6 order-2 order-lg-1 d-flex flex-column justify-content-center text-center text-lg-start">
                    <!-- 標題 -->
                    <h2>
                        歡迎來到
                        <span>Rasengan+</span>
                    </h2>
                    <!-- 描述 -->
                    <p>
                        們提供全方位訓練服務，從資訊軟體、技術、產品、硬體， 到前瞻趨勢、科技與管理等訓練相關業務，
                        以專業與跨域人才培育，縮短產學落差，提升人才素質，
                        以創新與前瞻學習服務，擴大產學合作，增能數位轉型
                    </p>
                    <div class="d-flex justify-content-center justify-content-lg-start">
                        <!-- 圓矩形按鈕連結 -->
                        <a href="#" class="btn-get-started">學員登入</a>
                        <!-- 影片播放按鈕 -->
                        <a href="https://www.youtube.com/watch?v=_y0ejTz64CM" class="glightbox btn-watch-video d-flex align-items-center"><i class="bi bi-play-circle"></i><span>觀看影片</span></a>
                    </div>
                </div>
                <!-- 注目區圖片 -->
                <div class="col-lg-6 order-1 order-lg-2">
                    <img src="~/assets/img/hero02.svg" class="img-fluid" alt="" data-aos="zoom-out" data-aos-delay="100">
                </div>
            </div>
        </div>

        <!-- 圖標方塊 - 連結（4個，注意動畫延遲屬性的設定） -->
        <div class="icon-boxes position-relative">
            <div class="container position-relative">
                <div class="row gy-4 mt-5">
                    <!-- data-aos-delay 動畫延遲屬性，單位：毫秒 -->
                    <div class="col-xl-3 col-md-6" data-aos="fade-up" data-aos-delay="100">
                        <div class="icon-box">
                            <div class="icon">
                                <i class="bi bi-person-heart"></i>
                            </div>
                            <h4 class="title">
                                <!-- <a href="#" class="stretched-link">圖標方塊 - 留在當頁的寫法</a> -->
                                <a href="#services" class="stretched-link">學員服務</a>
                            </h4>
                        </div>
                    </div>

                    <div class="col-xl-3 col-md-6" data-aos="fade-up" data-aos-delay="200">
                        <div class="icon-box">
                            <div class="icon">
                                <i class="bi bi-postcard-heart"></i>
                            </div>
                            <h4 class="title">
                                <a href="#testimonials" class="stretched-link">課程總覽</a>
                            </h4>
                        </div>
                    </div><!--End Icon Box -->

                    <div class="col-xl-3 col-md-6" data-aos="fade-up" data-aos-delay="300">
                        <div class="icon-box">
                            <div class="icon">
                                <i class="bi bi-envelope-paper-fill"></i>
                            </div>
                            <h4 class="title">
                                <a href="#call-to-action" class="stretched-link">最新消息</a>
                            </h4>
                        </div>
                    </div><!--End Icon Box -->

                    <div class="col-xl-3 col-md-6" data-aos="fade-up" data-aos-delay="500">
                        <div class="icon-box">
                            <div class="icon">
                                <i class="bi bi-chat-left-dots"></i>
                            </div>
                            <h4 class="title">
                                <a href="#about" class="stretched-link">關於我們</a>
                            </h4>
                        </div>
                    </div><!--End Icon Box -->
                </div>
            </div>
        </div>
    </section>
}
<hr />
