@model Class_system_Backstage_pj.Models.T課程科目分類

<div class="p-4 partial_details">

    <dl >

        <div class="row">
            <dt class="col-md-12">
                @Html.DisplayNameFor(model => model.科目類別名稱):
            </dt>       
        </div>
        <div class="row">
            <dd class="col-md-12">
                @Html.DisplayFor(model => model.科目類別名稱)
            </dd>
        </div>

        <div class="row">
            <dt class="col-md-12">
                @Html.DisplayNameFor(model => model.科目類別封面):
            </dt>           
        </div>

        <div class="row">
            <dd class="col-md-12">
                <img id="details_categoryImage" src="@Url.Content("~/images/t課程科目類別/" + @Model.科目類別封面)" style="height: 250px;" class="w-100 pt-2" />
            </dd>
        </div>

    </dl>
</div>



@section Scripts {
    @{
  
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
